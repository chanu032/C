#include <stdio.h>

int main()
{
    int x=20;
    int y=10;

    float sum, diff, mul, div;

    sum = x+y;
    diff = x-y;
    mul = x*y;
    div = x/y;

    printf("두 수의 합:%f\n", sum);
    printf("두 수의 차:%f\n", diff);
    printf("두 수의 곱:%f\n", mul);
    printf("두 수의 몫:%f\n", div);

    return 0;

}
